  addScore(false);\n        }\n        \n        disableButtons();\n    }\n\n    const handleContinue = () => {\n        addScore(true);\n        \n        window.dispatchEvent(new KeyboardEvent('keydown', {\n            key: \"s\",\n            keyCode: 83,\n            code: \"KeyS\",\n            which: 83\n        }));\n\n        enableButtons();\n        setIsCorrect(null);\n    }\n\n    const disableButtons = () => {\n        const buttons = document.querySelectorAll('button');\n        buttons.forEach((option) => {\n            option.setAttribute('disabled', 'true');\n            option.style.cursor = 'auto';\n            option.style.backgroundColor = 'white';\n            option.style.color = 'black';\n        });\n    }\n\n    const enableButtons = () => {\n        const buttons = document.querySelectorAll('button');\n        buttons.forEach((option) => {\n            option.removeAttribute('disabled');\n            option.style.cursor = 'pointer';\n        });\n    }\n\n    return (\n        <C.container>\n            {trigger ?\n                <>\n                {loading ? <C.loading>Loading...</C.loading> : \n                    <>\n                        <C.question>\n                            <h1>{question!.question}</h1>\n                        </C.question>\n                        <C.block>\n                            <CodeBlock\n                                language={language}\n                                showLineNumbers={true}\n                                text={question!.block}\n                                theme={dracula}\n                                \n                            />\n                        </C.block>\n                        <C.optionsContainer>\n                            {question!.options.map((option, index) => (\n                                <C.option key={index} onClick={() => {checkAnswer(option)}}>{option}</C.option>\n                            ))}\n                        </C.optionsContainer>\n                        {isCorrect === true ? \n                            <C.nextContainer>\n                                <C.correct>Correct!</C.correct>\n                                <C.continueButton onClick={handleContinue}>Continue</C.continueButton>\n                            </C.nextContainer> \n                        : ((isCorrect === false) ? \n                            <C.nextContainer>\n                                <C.incorrect>Incorrect!</C.incorrect> \n                            </C.nextContainer>\n                            : null)\n                        }\n                    </>\n                }\n                </>\n                :\n                <Guide/>\n            }\n        </C.container>\n    );\n}","/Users/luismelendez/Desktop/capstone/capstone/client/src/components/Question/QuestionStyling.ts",[],"/Users/luismelendez/Desktop/capstone/capstone/client/src/components/HowToPlay/HowToPlay.tsx",[],"/Users/luismelendez/Desktop/capstone/capstone/client/src/components/HowToPlay/HowToPlayStyling.ts",[],"/Users/luismelendez/Desktop/capstone/capstone/client/src/components/Guide/Guide.tsx",[],"/Users/luismelendez/Desktop/capstone/capstone/client/src/components/Guide/GuideStyling.ts",[],"/Users/luismelendez/Desktop/capstone/capstone/client/src/components/Leaderboard/LeaderboardStyling.ts",[],"/Users/luismelendez/Desktop/capstone/capstone/client/src/components/UploadScore/UploadScoreStyling.ts",[],"/Users/luismelendez/Desktop/capstone/capstone/client/src/components/UploadScore/UploadScore.tsx",[],"/Users/luismelendez/Desktop/capstone/capstone/client/src/components/Map/Map.tsx",["112"],"import * as C from './MapStyling';\nimport { useEffect } from 'react';\nimport { Character } from '../Character/Character';\nimport { CharacterSides } from \"../Character/CharacterSides\";\n\ntype props = {\n    difficulty: string,\n    character: {\n        trigger: boolean,\n        x: number,\n        y: number,\n        side: CharacterSides,\n        moveLeft(): void,\n        moveRight(): void,\n        moveUp(): void,\n        moveDown(): void\n    }\n}\n\nexport const Map = ({ difficulty, character }: props) => {\n\n    const handleKeyDown = (event: KeyboardEvent) => {\n        switch(event.code) {\n            case 'KeyW':\n                character.moveUp();\n                break;\n            case 'KeyA':\n                character.moveLeft();\n                break;\n            case 'KeyS':\n                character.moveDown();\n                break;\n            case 'KeyD':\n                character.moveRight();\n                break;\n        }\n    }\n\n    useEffect(() => {\n        window.addEventListener('keydown', handleKeyDown);\n    }, [])\n\n    return (\n        <C.map difficulty={difficulty}>\n            <Character \n            x = {character.x}\n            y = {character.y}\n            side = {character.side}\n            />\n        </C.map>\n    );\n}","/Users/luismelendez/Desktop/capstone/capstone/client/src/components/Map/MapStyling.ts",[],"/Users/luismelendez/Desktop/capstone/capstone/client/src/components/Leaderboard/Leaderboard.tsx",["113"],"import * as C from './LeaderboardStyling';\nimport { useState, useEffect } from 'react';\nimport axios from 'axios';\n\ntype props = {\n    \n}\n\nexport const Leaderboard = () => {\n    const [leaderboard, setLeaderboard] = useState<null | [{username: string, score: string}]>(null);\n\n    useEffect(() => {\n        axios.get('http://localhost:8080/leaderboard')\n        .then((response) => {\n            setLeaderboard(response.data);\n        })\n        .catch((err) => {\n            console.log(err);\n        });\n    }, [])\n\n    return (\n        <C.container>\n            <C.title>Leaderboard</C.title>\n            <C.header>\n                <C.headerText>Username</C.headerText>\n                <C.headerText>Score</C.headerText>\n            </C.header>\n            {(leaderboard && leaderboard.map((user, index) => 